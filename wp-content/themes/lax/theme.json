{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"duotone": [],
			"customDuotone": false,
			"defaultDuotone": false,
			"gradients": [],
			"customGradient": false,
			"defaultGradients": false,
			"palette": []
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1024px"
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"dropCap": false,
			"defaultFontSizes": false,
			"fontSizes": [
				{
					"name": "Small",
					"size": "13px",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "16px",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "24px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "32px",
					"slug": "x-large"
				}
			]
		}
	}
}
